      "components/*": ["./src/components/*"],
      "constants/*": ["./src/constants/*"],
      "network/*": ["./src/network/*"],
      "helpers/*": ["./src/helpers/*"],
      "actions/*": ["./src/redux/actions/*"],
      "redux/*": ["./src/redux/*"]
